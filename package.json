{
    "name": "brouter",
    "displayName": "BRouter Syntax Highlighting",
    "description": "BRouter Bike Routing Software - Profile Syntax highlighting",
    "publisher": "GregorH",
	"repository": {
		"type": "git",
		"url": "https://github.com/GregorH/vscode-language-brouter.git"
	},
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "brf",
            "aliases": ["BRouter", "brf"],
            "extensions": [".brf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "brf",
            "scopeName": "source.BRouter",
            "path": "./syntaxes/BRouter.tmLanguage.plist"
        }]
    },
    "license": "GPL-3.0",
    "__metadata": {
        "publisherDisplayName": "Gregor Heinrich"
    }

}
